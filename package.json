    "firebase-functions": "^4.2.0",
    "lodash": "^4.17.21",
    "moment": "2.29.4",
    "mongoose": "^7.0.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
